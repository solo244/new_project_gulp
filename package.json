{
  "name": "new_project_gulp",
  "folder": "new_project_gulp",
  "version": "1.5.0",
  "description": "A general template for new gulp projects",
  "main": "gulpfile.js",
  "author": "ken van damme",
  "copyright": "solo244",
  "google": "UA-XXXXXXXX-X",
  "paths": {
    "assets": {
      "main": "dev/",
      "sass": "dev/css/",
      "js": "dev/js/",
      "images": "dev/images/",
      "pug": "dev/views/"
    },
    "build": {
      "main": "build/",
      "css": "build/css/",
      "js": "build/js/",
      "images": "build/images/"
    },
    "dist": {
      "main": "dist/",
      "online_folder": ""
    }
  },
  "vars": {
    "sass": "main.scss",
    "css": "style.min.css",
    "js": "main.min.js"
  },
  "url": {
    "live": "https://livewebsite.com",
    "local": "http://localhost/"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "lightbox2": "^2.11.1",
    "vanilla-lazyload": "^12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "cssnano": "^4.1.10",
    "del": "^5.0.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-filter": "^6.0.0",
    "gulp-imagemin": "^6.1.0",
    "gulp-load-plugins": "^2.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
